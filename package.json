{
  "name": "swagger-typescript-codegen",
  "main": "./lib/codegen.js",
  "version": "3.2.0",
  "description": "A Swagger Codegenerator tailored for typescript.",
  "scripts": {
    "pretest": "npm run build",
    "test": "grunt && jest",
    "clean": "rm -rf tmp-*",
    "build": "tsc --project tsconfig.json",
    "build:watch": "npm run build -- --watch",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "bin": {
    "swagger2ts": "bin/swagger2ts.js"
  },
  "bugs": {
    "url": "https://github.com/mtennoe/swagger-typescript-codegen/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mtennoe/swagger-typescript-codegen.git"
  },
  "keywords": [
    "swagger",
    "rest"
  ],
  "author": {
    "name": "Marius Tenn√∏e",
    "email": "mariust@outlook.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://github.com/mtennoe/swagger-typescript-codegen",
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^23.3.10",
    "@types/js-beautify": "1.8.0",
    "@types/lodash": "^4.14.119",
    "@types/mustache": "^0.8.32",
    "@types/node": "^10.12.18",
    "commander": "^2.19.0",
    "final-fs": "^1.6.0",
    "grunt": "^1.0.3",
    "grunt-contrib-jshint": "^2.0.0",
    "grunt-jsonlint": "^1.0.4",
    "grunt-vows": "^0.4.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "js-beautify": "^1.8.9",
    "jshint": "^2.9.7",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "matchdep": "^2.0.0",
    "mustache": "^3.0.1",
    "prettier": "1.15.3",
    "request": "^2.88.0",
    "superagent": "^4.0.0",
    "tmp": "0.0.33",
    "typescript": "^3.2.2",
    "update-notifier": "^2.5.0",
    "vows": "^0.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,md},": [
      "prettier --write",
      "git add"
    ]
  }
}
